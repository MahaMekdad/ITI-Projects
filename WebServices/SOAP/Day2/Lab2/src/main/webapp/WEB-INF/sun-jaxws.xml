<?xml version="1.0" encoding="UTF-8"?>

<endpoints xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime" version="2.0">
    <endpoint
            name="EmployeeWebService"
            implementation="org.example.web.services.impls.EmployeeServiceImpl"
            url-pattern="/ws/employee">
            <handler-chains xmlns="https://jakarta.ee/xml/ns/jakartaee">
                <handler-chain>
     
                    <handler>
                        <handler-class>org.example.handlers.MyLogicalHandler</handler-class>
                    </handler>
     
                    <handler>
                        <handler-class>org.example.handlers.MySOAPHandler</handler-class>
                    </handler>
     
                </handler-chain>
            </handler-chains>
            
    </endpoint>

</endpoints>